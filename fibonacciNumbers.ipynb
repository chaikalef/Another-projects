   "source": [
    "inNum = int(input(\"Enter number: \"))\n",
    "\n",
    "def fib(num):\n",
    "    if (num == 2) | (num == 1):\n",
    "        return 1\n",
    "    elif num <= 0:\n",
    "        return 0\n",
    "    else:\n",
    "        return fib(num - 1) + fib(num - 2)\n",
    "\n",
    "print(str(inNum) + \"! = \" + str(fib(inNum)))"
   ]
  }
